{
    "version": 2,
    "functions": {
        "api/*.js": {
            "maxDuration": 60
        },
        "api/auth/*.js": {
            "maxDuration": 60
        },
        "api/user/*.js": {
            "maxDuration": 60
        },
        "api/task/*.js": {
            "maxDuration": 60
        }
    },
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "output": "dist",
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ]
}